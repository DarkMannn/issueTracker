{
    "plugins": ["jsdoc-route-plugin"],
    "recurseDepth": 3,
    "source": {
        "includePattern": ".+\\.js(doc|x)?$",
		"excludePattern": "(^|\\/|\\\\)_",
		"include": [ "." ],
		"exclude": [ "node_modules" ]
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts":{
		"template":"./node_modules/minami"
    }
}
